{
    "manifest_version": 3,
    "name": "Model Buddy",
    "description": "Capture and redirect 3D model downloads for Model Scout Application",
    "version": "1.0",
    "permissions" : ["downloads", "storage"],
    "background": {
        "service_worker" : "background.js"
    },
    "action": {
        "default_popup": "controls.html"
      }
  }